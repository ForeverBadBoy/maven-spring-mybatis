<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	 <settings>
        <!-- 打印查询语句 -->
        <setting name="logImpl" value="STDOUT_LOGGING" />
    </settings>
    
	<!-- 类型别名-->
	<typeAliases>
		<typeAlias alias="User" type="cn.com.kid.pojo.User" />
	</typeAliases>
 	<!-- 映射map -->    
 	<mappers>
 	    <mapper resource="cn/com/kid/mapping/UserMapper.xml"/> 
 	    <!-- <mapper class="cn.com.kid.pojo.User"/> -->
 	    <mapper resource="cn/com/kid/mapping/UserModelMapper.xml"></mapper>
 	</mappers>
</configuration>